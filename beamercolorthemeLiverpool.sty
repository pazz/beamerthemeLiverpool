% Settings
\definecolor{wwgrey}{HTML}{3f4246}





\colorlet{wwmaincolour}{Atlantic Blue}
\colorlet{wwsurroundcolour}{PresentationBlue}
\colorlet{wwcontrastcolor}{white}



\setbeamercolor*{title}{fg=white}
\setbeamercolor*{date}{fg=white}
\setbeamercolor*{page header}{fg=blue}
\setbeamercolor*{author}{fg=white}
\setbeamercolor*{institute}{fg=white}

\setbeamercolor*{frametitle}{fg=Classic Black}


\setbeamercolor{palette primary}{fg=wwgrey}
\setbeamercolor{palette secondary}{fg=wwgrey}
\setbeamercolor{palette tertiary}{fg=wwgrey}
\setbeamercolor{palette quaternary}{fg=wwgrey}
\setbeamercolor{normal text}{fg=wwgrey}



\setbeamercolor{block title example}{use=structure,fg=Classic Black,bg=white}
\setbeamercolor{block body example}{use=structure,bg=white,fg=wwgrey}

\setbeamercolor{block title definition}{use=structure,fg=Classic Black,bg=white}
\setbeamercolor{block body definition}{use=structure,bg=white,fg=wwgrey}


\setbeamercolor{block title}{use=structure,fg=Classic Black,bg=white}
\setbeamercolor{block body}{use=structure,bg=white,fg=wwgrey}


\setbeamercolor{coloredboxstuff}{fg=wwcontrastcolor,bg=white!10!wwsurroundcolour}

\setbeamercolor{section in head/foot}{bg=wwsurroundcolour!80,fg=wwmaincolour}
\setbeamercolor{author in head/foot}{bg=wwsurroundcolour,fg=wwcontrastcolor}
\setbeamercolor{date in head/foot}{bg=wwsurroundcolour!70,fg=wwcontrastcolor}
\setbeamercolor{title in head/foot}{bg=wwsurroundcolour!90,fg=wwcontrastcolor}

\setbeamertemplate{itemize item}{\scriptsize\raise1.25pt\hbox{\color{wwmaincolour}{$\blacktriangleright$}}}
