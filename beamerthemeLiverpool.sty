\mode<presentation>

% Requirement
\RequirePackage{tikz}
\RequirePackage[skins]{tcolorbox}
\RequirePackage{etex}

% Settings
\usecolortheme{Liverpool}

\useinnertheme{circles}
\useinnertheme{Liverpool}

\useoutertheme{Liverpool}

% Maybe later?
%\RequirePackage{poppins}
%\usefonttheme{professionalfonts} % using non standard fonts for beamer
%\usefonttheme{Liverpool}


\setbeamertemplate{navigation symbols}{}
%\setbeamertemplate{blocks}[rounded][shadow=true]  


% Macro for quickly overloading colours
\NewDocumentCommand{\uolcolours}{m m}{
\setbeamercolor*{structure}{fg=#2}

\setbeamercolor*{block title}{fg=#2}
\setbeamercolor*{block body}{fg=#2}

% alerted text and title of alertblock
\setbeamercolor*{alerted text}{fg=Pink}

\setbeamercolor{normal text}{bg=#1,fg=#2}
\usebeamercolor[fg]{normal text}
}


\mode<all>
