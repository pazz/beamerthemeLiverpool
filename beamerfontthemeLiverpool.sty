\RequirePackage[T1]{fontenc}
\RequirePackage{poppins} 

\setbeamerfont{normal text}{series=\fontPoppins}
\setbeamerfont{section name}{series=\fontPoppins}
\setbeamerfont{section page}{series=\fontPoppins}
\setbeamerfont{section in page}{series=\fontPoppins}
\setbeamerfont{section in sidebar}{series=\fontPoppins}
\setbeamerfont{section in head/foot}{series=\fontPoppins}
\setbeamerfont{projected text}{series=\fontPoppins}
\setbeamerfont{structure}{series=\fontPoppins}

\setbeamerfont{tiny structure}{series=\fontPoppins}
\setbeamerfont{frametitle}{size=\large,series=\fontPoppins} % ,series=\bfseries
\setbeamerfont{framesubtitle}{size=\footnotesize,series=\fontPoppins}
\setbeamerfont{footline}{size=\Tiny,series=\fontPoppins}
\setbeamerfont{title}{series=\bfseries}
\setbeamerfont{subtitle}{series=\fontPoppins}

\setbeamerfont{item}{series=\fontPoppins}
\setbeamerfont{item projected}{series=\fontPoppins}
\setbeamerfont{itemize item}{series=\fontPoppins}
\setbeamerfont{itemize subitem}{series=\fontPoppins}
\setbeamerfont{itemize subsubitem}{series=\fontPoppins}
\setbeamerfont{itemize/enumerate body}{series=\fontPoppins}
\setbeamerfont{itemize/enumerate subbody}{series=\fontPoppins}
\setbeamerfont{itemize/enumerate subsubbody}{series=\fontPoppins}

\AtBeginDocument{\usebeamerfont{normal text}}
